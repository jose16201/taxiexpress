<h3>Lista de Rutas</h3>

<table>
  <tr>
    <th>Distrito Origen</th>
    <th>Distrito Destino</th>
    <th>Tarifa</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sectors.each do |sector| %>
  <tr>
    <td><%= sector.distrito_origen %> &nbsp; &nbsp;&nbsp;</td>
     <td><%= sector.distrito_destino %> &nbsp; &nbsp;&nbsp;</td>
    <td><%= sector.costo_tarifa %> &nbsp; &nbsp;&nbsp;</td>
    <td><%= link_to 'Show', sector %></td>
    <td><%= link_to 'Edit', edit_sector_path(sector) %></td>
    <td><%= link_to 'Delete', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sector', new_sector_path %>
